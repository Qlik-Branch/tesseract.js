{
  "name": "qlik-hypercube-definition-generator",
  "version": "1.0.0",
  "description": "A library to quickly generate a JSON hypercube definition",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx tslint --project .",
    "coverage": "npx nyc npm test | npx coveralls",
    "test": "npx ts-mocha -p ./tsconfig.json test/**/*.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*",
      "dist/**/*",
      "coverage/**/*"
    ],
    "reporter": [
      "text-lcov"
    ],
    "all": true
  },
  "author": "rjriel",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-mocha": "^2.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.4"
  }
}
